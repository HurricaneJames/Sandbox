{
  "name": "react-drag",
  "description": "a basic react project for testing drag and drop components",
  "main": "index.js",
  "version": "1.0.0",
  "devDependencies": {
    "browserify": "^6.3.3",
    "watchify": "^2.1.1",
    "react": "^0.12.0",
    "reactify": "^0.17.1",
    "http-server": ""
  },
  "browserify": {
    "transform": ["reactify"]
  },
  "scripts": {
    "build": "browserify --transform reactify browser.js > static/bundle.js",
    "watch": "watchify browser.js -o static/bundle.js --debug --verbose"
  }
}